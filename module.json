{
  "name": "osr-character-builder",
  "title": "OSR Character Builder",
  "description": "A character builder for OSE",
  "author": "Grim",
  "version": "0.1.11",
  "minimumCoreVersion": "0.8.8",
  "compatibleCoreVersion": "9.255",
  "styles": ["./styles/main.css"],
  "esmodules": [
    "./scripts/init.js"
  ],
  "packs": [
    {
      "name": "osr-srd-class-options",
      "label": "OSE SRD Class Options",
      "path": "packs/osr-srd-class-options.db",
      "entity": "Item",
      "module": "osr-character-builder"
    },
    {
      "name": "osr-srd-items",
      "label": "OSE SRD items",
      "path": "packs/osr-srd-items.db",
      "entity": "Item",
      "module": "osr-character-builder"
    },
    {
      "name": "osr-srd-spells",
      "label": "OSE SRD Spells",
      "path": "packs/osr-srd-spells.db",
      "entity": "Item",
      "module": "osr-character-builder"
    },
    {
      "name": "osr-srd-classes",
      "label": "OSE SRD Classes",
      "path": "packs/osr-srd-classes.db",
      "entity": "JournalEntry",
      "module": "osr-character-builder"
    },
    {
      "name": "osr-character-builder-macros",
      "label": "osr-character-builder Macros",
      "path": "packs/macros.db",
      "entity": "Macro",
      "module": "osr-character-builder"
    }
  ],
  "dependencies": [
    {
      "name": "compendium-folders",
      "manifest": "https://raw.githubusercontent.com/earlSt1/vtt-compendium-folders/0.1.1/module.json",
      "type": "module"
    },
    {
      "name": "lib-wrapper",
      "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json",
      "type": "module"
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "url": "https://github.com/RabidOwlbear/osr-character-builder",
  "manifest": "https://raw.githubusercontent.com/RabidOwlbear/osr-character-builder/main/module.json",
  "download": "https://github.com/RabidOwlbear/osr-character-builder/archive/refs/heads/main.zip",
  "readme": "README.md"
}
