      "module": "OSE-CharacterBuilder"
    },
    {
      "name": "OSE-SRD-items",
      "label": "OSE SRD items",
      "path": "packs/OSE-SRD-items.db",
      "entity": "Item",
      "module": "OSE-CharacterBuilder"
    },
    {
      "name": "OSE-SRD-spells",
      "label": "OSE SRD Spells",
      "path": "packs/OSE-SRD-spells.db",
      "entity": "Item",
      "module": "OSE-CharacterBuilder"
    },
    {
      "name": "OSE-SRD-classes",
      "label": "OSE SRD Classes",
      "path": "packs/OSE-SRD-classes.db",
      "entity": "JournalEntry",
      "module": "OSE-CharacterBuilder"
    },
    {
      "name": "OSE-CharacterBuilder-macros",
      "label": "OSE-CharacterBuilder Macros",
      "path": "packs/macros.db",
      "entity": "Macro",
      "module": "OSE-CharacterBuilder"
    }
  ],
  "dependencies": [
    {
      "name": "compendium-folders",
