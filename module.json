{
  "name": "OSE-CharacterBuilder",
  "title": "OSE Character Builder",
  "description": "A character builder for OSE",
  "author": "Grim",
  "version": "0.1.7",
  "minimumCoreVersion": "0.8.8",
  "compatibleCoreVersion": "0.9.0",
  "styles": ["./styles/main.css"],
  "scripts": [
    "./scripts/init.js", 
    "./scripts/charBuilder.js", 
    "./scripts/SRDClassData.js",
    "./scripts/retainerBuilder.js"
